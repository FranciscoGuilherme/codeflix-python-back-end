{
	"name": "Micro Videos Python",
	"dockerComposeFile": [
		"../docker-compose.yaml",
		"docker-compose.yml"
	],
	"service": "app",
	"workspaceFolder": "/home/python/app",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"sonarsource.sonarlint-vscode"
			]
		},
		"settings": {
			"python.pythonPath": "/usr/local/bin/python",
			"terminal.integrated.defaultProfile.linux": "zsh"
		}
	},
	"remoteUser": "python"
}
